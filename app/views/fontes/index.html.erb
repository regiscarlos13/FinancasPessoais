
<% @card_title = "Relação de Fonte" %>
<% @btn_new = btn_new(new_fonte_path) %>

<div class="table-responsive">
  <%= table_for(@fontes) do %>
    <% field_table('Documento') { |f| f.documento } %>
    <% field_table('Nome') { |f| f.nome} %>
    <% field_table('Telefone') { |f| f.telefone} %>
    <% field_table('Situação') { |f| situacao(f.ativo)} %>
    <% btn_for %>

  <% end %>
</div>
